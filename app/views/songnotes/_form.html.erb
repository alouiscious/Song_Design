

  <%= form_for @songnote do |f| %>
  <%= f.label :song_title %>
  <%= f.text_field :song_title, list: "songs_list" %>
    <datalist id="songs_list">
      <% Song.all.each do |song| %>
        <option value="<%= song.title %>">
      <% end %>
    </datalist>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :content %>
  <%= f.text_area :content, rows: 7, cols: 28 %> <br>
  <br>

  <%= f.submit %>
<br>
<br>
<% end %>