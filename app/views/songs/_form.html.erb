
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :key %>
  <%= f.text_field :key %> <br>
  <%= f.label :genre %>
  <%= f.text_field :genre %> <br>
  <%= f.label :in_style_of %>
  <%= f.text_field :in_style_of %> <br>
  <%= f.radio_button(:status, "true") %>
  <%= f.label(:status_inuse, "In Use") %>
  <%= f.radio_button(:status, "false") %> 
  <%= f.label(:status_notinuse, "Not In Use") %>
<br>
<br>


   
  <%= f.submit %>
<% end %>
<br>
<br>

<%= button_to "Add Song Design Note", new_songnote_path(song) %>
