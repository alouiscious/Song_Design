
<%= form_for :user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :instrument %>
  <%= f.text_field :instrument %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <br>
  <%= f.submit %>
  
<% end %>